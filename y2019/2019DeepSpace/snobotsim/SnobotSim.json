                "com.snobot.simulator:adx_family:${version_number}",
                "com.snobot.simulator:navx_simulator:${version_number}",
                
                "edu.wpi.first.ntcore:ntcore-jni:${wpilibVersion}:${nativeclassifier}"
            ],
            "jni": [
                "edu.wpi.first.wpiutil:wpiutil-cpp:${wpilibVersion}:${nativeclassifier}@zip",
                "edu.wpi.first.hal:hal-cpp:${wpilibVersion}:${nativeclassifier}@zip",
                "com.snobot.simulator:adx_family:${version_number}:${nativeclassifier}",
                "com.snobot.simulator:navx_simulator:${version_number}:${nativeclassifier}",
