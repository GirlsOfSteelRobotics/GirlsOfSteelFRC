{
    "maven_repos": {
        "SnobotSim": "http://raw.githubusercontent.com/pjreiniger/maven_repo/master/",
        "Wpi": "http://first.wpi.edu/FRC/roborio/maven/release/"
    },
    "required_third_party": [
        "CTRE-Phoenix",
        "REVRobotics"
