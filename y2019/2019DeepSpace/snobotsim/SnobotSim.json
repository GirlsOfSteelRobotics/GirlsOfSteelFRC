                "edu.wpi.first.thirdparty.frc2019.opencv:opencv-cpp:${wpiOpenCvVersion}:${nativeclassifier}@zip"
            ],
            "cpp": [
                "com.snobot.simulator:snobot_sim:${version_number}",
                "com.snobot.simulator:adx_family:${version_number}",
                "com.snobot.simulator:navx_simulator:${version_number}"
            ],
            "cpp_java": [
                "edu.wpi.first.wpilibj:wpilibj-java:${wpilibVersion}",
                "edu.wpi.first.ntcore:ntcore-java:${wpilibVersion}",
                "edu.wpi.first.cscore:cscore-java:${wpilibVersion}",
