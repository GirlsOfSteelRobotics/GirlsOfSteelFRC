<?import javafx.scene.shape.*?>

<Pane fx:id="m_pane" xmlns="http://javafx.com/javafx/9"
    xmlns:fx="http://javafx.com/fxml/1"
    fx:controller="com.gos.codelabs.shuffleboard.sd_widgets.ss.CodelabSuperStructureController">

    <Group fx:id="m_group">
        <children>
            <Rectangle fx:id="m_base" fill = "peachPuff"/>
            <Rectangle fx:id="m_elevatorBox" fill = "papayawhip"/>
            <Rectangle fx:id="m_liftUpperLimitSwitch" fill = "mediumpurple"/>
            <Rectangle fx:id="m_liftLowerLimitSwitch" fill = "mediumpurple"/>
            <Rectangle fx:id="m_punch" fill = "lightpink"/>
            <Circle fx:id="m_spinningWheel" fill = "mediumvioletred"/>
        </children>
